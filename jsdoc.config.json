{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "destination": "./docs/jsdoc-html"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "idInHeadings": true
  }
}
